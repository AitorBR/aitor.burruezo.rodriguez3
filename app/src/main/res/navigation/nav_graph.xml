<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/FirstFragment">

    <fragment
        android:id="@+id/FirstFragment"
        android:name="cat.copernic.finall.aitor.burruezo.rodriguez.FirstFragment"
        android:label="@string/first_fragment_label"
        tools:layout="@layout/fragment_first">

        <action
            android:id="@+id/action_FirstFragment_to_SecondFragment"
            app:destination="@id/SecondFragment" />
        <action
            android:id="@+id/action_FirstFragment_to_thirdFragment"
            app:destination="@id/thirdFragment" />
    </fragment>
    <fragment
        android:id="@+id/SecondFragment"
        android:name="cat.copernic.finall.aitor.burruezo.rodriguez.SecondFragment"
        android:label="@string/second_fragment_label"
        tools:layout="@layout/fragment_second">

        <action
            android:id="@+id/action_SecondFragment_to_FirstFragment"
            app:destination="@id/FirstFragment" />
        <action
            android:id="@+id/action_SecondFragment_to_second1Fragment"
            app:destination="@id/second1Fragment" />
    </fragment>
    <fragment
        android:id="@+id/thirdFragment"
        android:name="cat.copernic.finall.aitor.burruezo.rodriguez.ThirdFragment"
        android:label="fragment_third"
        tools:layout="@layout/fragment_third" >
        <action
            android:id="@+id/action_thirdFragment_to_FirstFragment"
            app:destination="@id/FirstFragment" />
        <action
            android:id="@+id/action_thirdFragment_to_third1Fragment"
            app:destination="@id/third1Fragment" />
    </fragment>
    <fragment
        android:id="@+id/second1Fragment"
        android:name="cat.copernic.finall.aitor.burruezo.rodriguez.Second1Fragment"
        android:label="fragment_second1"
        tools:layout="@layout/fragment_second1" />
    <fragment
        android:id="@+id/third1Fragment"
        android:name="cat.copernic.finall.aitor.burruezo.rodriguez.Third1Fragment"
        android:label="fragment_third1"
        tools:layout="@layout/fragment_third1" />
</navigation>